const mongoose = require("mongoose");

const UserSchema = new mongoose.Schema({

    name:{
        type: String,
    },
    email:{
        type:String,
        require: true,
        unique:true,
    },
    password:{
        type: String,
        require: true,
    },
    age: {
        type: Number,
        enum:['Male', 'Female']
    }
});

const User = mongoose.model('user', UserSchema);

module.exports = { User};